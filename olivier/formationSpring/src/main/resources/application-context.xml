<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	
	<bean id="amine" class="formationSpring.Employe">
		<property name="nom" value="amine"></property>
		<property name="departement" ref="info"></property>
		<property name="poste" ref="manager"></property>
	</bean>

	<bean id="info" class="formationSpring.Departement">
		<property name="nom" value="compta"></property>
	</bean>
	
	<bean id="manager" class="formationSpring.Poste">
		<property name="nom" value="manager"></property>
	</bean>
</beans>




	



